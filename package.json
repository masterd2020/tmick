{
	"name": "tmick",
	"version": "0.0.0",
	"private": true,
	"workspaces": [
		"packages/*"
	],
	"scripts": {
		"build": "lerna run build",
		"clean": "lerna run clean",
		"test": "jest",
		"lint": "eslint . --ext .ts,.tsx",
		"lint:fix": "npx eslint . --ext .ts,.tsx --fix",
		"format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
		"publish-packages": "lerna publish --conventional-commits --yes --no-private"
	},
	"keywords": [
		"dependency injection",
		"dependency",
		"injection",
		"ioc",
		"container",
		"javascript",
		"cqrs",
		"command query responsibility segregation",
		"typescript"
	],
	"engines": {
		"node": ">= 20.0.0"
	},
	"author": "Fasasi Toheeb",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/masterd2020/tmick/issues"
	},
	"homepage": "https://github.com/masterd2020/tmick#readme",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/masterd2020/tmick.git"
	},
	"devDependencies": {
		"@types/jest": "^29.5.0",
		"@types/node": "^18.15.11",
		"@typescript-eslint/eslint-plugin": "^5.57.1",
		"@typescript-eslint/parser": "^5.57.1",
		"eslint": "^8.37.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-prettier": "^5.4.0",
		"jest": "^29.5.0",
		"lerna": "^8.2.2",
		"prettier": "^3.5.3",
		"ts-jest": "^29.1.0",
		"typescript": "^5.0.3"
	}
}
